#include "mt7628an_glinet_vixmini_microuter.dtsi"

/ {
	compatible = "glinet,vixmini", "mediatek,mt7628an-soc";
	model = "GL.iNet VIXMINI";
};

&led_power_blue {
	label = "vixmini:blue:power";
};

&led_wlan_white {
	label = "vixmini:white:wlan";
};

&firmware_part {
		reg = <0x50000 0x7b0000>;
};
